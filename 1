local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Menu VY09", "DarkTheme")

-- Cập nhật HumanoidRootPart sau khi nhân vật reset
local player = game.Players.LocalPlayer
local hrp = nil

local function CapNhatHRP()
    local char = player.Character or player.CharacterAdded:Wait()
    hrp = char:WaitForChild("HumanoidRootPart")
end

player.CharacterAdded:Connect(CapNhatHRP)
CapNhatHRP()

-- TAB Dịch chuyển
local Tab1 = Window:NewTab("Dịch chuyển")
local Section1 = Tab1:NewSection("Vị trí phổ biến")

-- Thêm nút bay tới toạ độ (bay từ từ)
Section1:NewButton("Đảo Sa Mạc", "Bay đến vị trí bí mật", function()
    if hrp then
        local target = Vector3.new(1216.343994140625, 32.102996826171875, 4366.68798828125) -- thay đổi tuỳ tọa độ bạn muốn
        local speed = 250 -- tốc độ bay

        local tweenService = game:GetService("TweenService")
        local tweenInfo = TweenInfo.new((hrp.Position - target).Magnitude / speed, Enum.EasingStyle.Linear)
        local tween = tweenService:Create(hrp, tweenInfo, {CFrame = CFrame.new(target)})
        tween:Play()
    end
end)

Section1:NewButton("Đảo Người Cá", "Bay tới vị trí spawn", function()
    if hrp then
        local spawnPos = Vector3.new(3856.487060546875, 4.4799957275390625, -1926.1629638671875) -- vị trí spawn bạn muốn
        local speed = 250

        local tweenService = game:GetService("TweenService")
